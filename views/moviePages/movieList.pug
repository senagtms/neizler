extends ../layout/onlinePageLayout

block content
    section
        .container-app
            .row.pt-5
                .col-md-3
                    include ../partials/adminMenu
                .col-md-9
                    table.table
                        thead
                            tr
                                th(scope='col') Film Adı
                                th(scope='col') Film Orjinal Adı
                                th(scope='col') Kategoriler
                                th(scope='col') Yıl
                                th(scope='col') Imdb
                                th(scope='col') Yönetmen
                                th(scope='col') Düzenle
                        tbody
                            each val in list
                                 tr
                                    td= val.title   
                                    td= val.originalTitle
                                    td
                                        each item in val.categoriesData
                                            span= item.title + ' '
                                    td= val.year 
                                    td= val.imdb
                                    td= val.directorData.name + ' ' + val.directorData.surName
                                    td
                                        a(href=href=`${myUrl}/admin/movie/update/`+val._id) Düzenle

                              
                                
                                  
                                              